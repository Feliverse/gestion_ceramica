<div id="<%= dom_id inventory_entry_detail %>">
  <p>
    <strong>Inventory entry:</strong>
    <%= inventory_entry_detail.inventory_entry_id %>
  </p>

  <p>
    <strong>Product:</strong>
    <%= inventory_entry_detail.product_id %>
  </p>

  <p>
    <strong>Quantity:</strong>
    <%= inventory_entry_detail.quantity %>
  </p>

  <p>
    <strong>Subtotal:</strong>
    <%= inventory_entry_detail.subtotal %>
  </p>

</div>
